@using (Html.BeginForm("DownloadViewPDF", "Home", FormMethod.Post))
{
    <span class="leftalign">
        <input type="submit" id="btnIndex" class="myButton" value="PDF" />
    </span>
}


<div class="center">
    <table>
        <tr>
            <th>Speed</th>
            <th>Datetime</th>
            <th>Car ID</th>
        </tr>
        <tr>
        @foreach (HtmlString item in ViewBag.speed)
        {
            @item
        }
        </tr>
    </table>
</div>